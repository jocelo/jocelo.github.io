  "php": {
    "code": [
      "",
      "  function balanced($the_string) {",
      "    $stack = [];",
      "    $len = strlen($the_string);",
      "",
      "    for ($i=0 ; $i<$len ; $i++) {",
      "      $par = $the_string[$i];",
      "",
      "      if (in_array($par, ['(', '{', '['])) {",
      "        array_push($stack, $par);",
      "      }",
      "",
      "      if ($par == ')') {",
      "        $parOpen = array_pop($stack);",
      "        if ($parOpen != '(') {",
      "          return false;",
      "        }",
      "      }",
      "",
      "      if ($par == '}') {",
      "        $parOpen = array_pop($stack);",
      "        if ($parOpen != '{') {",
      "          return false;",
      "        }",
      "      }",
      "",
      "      if ($par == ']') {",
      "        $parOpen = array_pop($stack);",
